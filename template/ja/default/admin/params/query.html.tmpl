[%
   title = "既定のクエリ"
   desc = "クエリと $terms.bug リストの既定値"
%]

[% param_descs = {
  quip_list_entry_control => "quip リストに追加するための権限の設定です。
                              <ul>
                                <li>
                                  open - ユーザは自由に追加でき、すぐに表示されるようになります。
                                </li>
                                <li>
                                  moderated - quip を追加できますが、管理者が有効にしなければなりません。
                                </li>
                                <li>
                                  closed - quip への新規追加はできません。
                                </li>
                              </ul>",

  mostfreqthreshold => "$terms.abug の重複がこの数以上であれば、" _
                       "<a href=\"duplicates.cgi\">最近のよく報告される $terms.bugs ページ</a>" _
                       "に表示されます。もし、巨大なデータベースでこのページを表示するのに時間が" _
                       "かかるようならば、数を増やしてください。",

  mybugstemplate => "'わたしの $terms.bug' リンクが指す URL に利用されます。" _
                    "%userid% はユーザのログイン名に置換されます。",

  defaultquery => "高度な検索ページを表示したときの初期表示の検索内容です。" _
                  "URL パラメータ形式ですので読みにくいですがご了承を !",

  specific_search_allow_empty_words => 
    "空の単語リストで 'バグの表示' から検索を許可するか" _
    "どうか。" 
}
%]

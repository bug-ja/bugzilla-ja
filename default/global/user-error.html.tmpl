[%# INTERFACE:
  # header_done: boolean. True if the Bugzilla header has already been printed.
  # error: string. The tag of the error, or the error message to be displayed
  # (deprecated). May contain HTML if it's an error message.
  #%]

[%# This is a list of all the possible user errors. Please keep them in
  # alphabetical order by error tag, and leave a blank line between errors.
  #
  # Note that you must explicitly filter every single template variable
  # in this file; if you do not wish to change it, use the "none" filter.
  #
  # Extension- or custom-specific error handling  can be easily added 
  # via hooks: just place your <extension>-errors.html.tmpl into 
  # template/en/extension/hook/global/user-error.html.tmpl/errors/
  # Note: be aware of uniqueness of error string parameter value, since 
  # nobody can guarantee the hook files processing order in the future
  #%]

[% PROCESS global/variables.none.tmpl %]

[% PROCESS "global/field-descs.none.tmpl" %]

[% DEFAULT title = "エラー" %]

[% error_message = BLOCK %]
  [% IF    error == "account_creation_disabled" %]
    [% title = "アカウント作成は無効化されています" %]
    ユーザアカウント作成は無効化されています。
    <hr>
    新規アカウント作成は管理者が行います。
    管理者は [% Param("maintainer") %] です。

  [% ELSIF error == "account_creation_restricted" %]
    [% title = "アカウント作成は制限されています" %]
    ユーザアカウント作成は制限されています。
    <hr>
    管理者もしくはメンテナー ([% Param("maintainer") %])
    にアカウント作成のための情報を問い合わせてください。

  [% ELSIF error == "account_disabled" %]
    [% title = "アカウントは無効です" %]
    [% disabled_reason FILTER none %]
    <hr>
    アカウントが再度、有効化されるべきだと思う場合、
    上記にある無効化された理由に対する妥当と判断可能な理由を添付して
    [% Param("maintainer") %] までメールで申請してください。

  [% ELSIF error == "account_exists" %]
    [% title = "指定のアカウントは既に存在します" %]
    指定された
    [% IF email %]
      ログイン名 ([% email FILTER html %]).
    [% ELSE %]
      ログイン名
    [% END %]
    のアカウントは既に存在しています。

  [% ELSIF error == "alias_has_comma_or_space" %]
    [% title = "エイリアスに無効な文字が含まれています" %]
    入力されたエイリアス <em>[% alias FILTER html %]</em> には一つ以上のカンマ、
    もしくはスペースが含まれています。これらの文字はリストの中でのセパレータとして利用されますので、
    名称としては利用できません。カンマやスペースを含まないエイリアス名をつけてください。

  [% ELSIF error == "alias_in_use" %]
    [% title = "エイリアスは既に利用されています" %]
    [% terms.Bug %] [%+ bug_id FILTER bug_link(bug_id) FILTER none %]
    は既に <em>[% alias FILTER html %]</em> というエイリアスを持ちます。
    別の名称を選択してください。

  [% ELSIF error == "alias_is_numeric" %]
    [% title = "エイリアスが数字です" %]
    この[% terms.bug %] にエイリアス <em>[% alias FILTER html %]</em> をつけようとしましたが、
    [% terms.bug %] ID と混同する可能性があるため、エイリアスは数字だけでは適用できません。
    少なくとも一文字は英文字を含むエイリアスを選択してください。

  [% ELSIF error == "alias_too_long" %]
    [% title = "エイリアスが長すぎます" %]
    [% terms.Bug %] のエイリアスは 20 文字 (byte) 以上にはできません。
    より短いものにしてください。

  [% ELSIF error == "auth_cant_create_account" %]
    [% title = "アカウントを作成できません" %]
    このサイトが利用している認証サービスは、アカウント作成を許可していません。
    新規アカウント作成については、管理者に問い合わせてください。

  [% ELSIF error == "auth_failure" %]
    [% title = "認証が必要です" %]
    [% admindocslinks = {'groups.html' => 'グループセキュリティー'} %]
    [% IF group %]
      あなたは、'[% group FILTER html %]' グループのメンバーではありません。
    [% END %]

    [% IF reason %]
      [% IF group %] また、 [% END %]
      [% IF reason == "cant_bless" %]
        グループへのアカウントの追加・削除権限がありません。
      [% ELSIF reason == "not_visible" %]
        指定されたユーザグループへの閲覧制限がかかっています。
      [% END %]
    [% END %]

    [% IF group || reason %] そして、 [% END %]

    [% IF object == "administrative_pages" %]
      管理画面
    [% ELSIF object == "attachment" %]
      この添付
    [% ELSIF object == "bugs" %]
      [%+ terms.bugs %]
    [% ELSIF object == "charts" %]
      新規チャートの追加機能
    [% ELSIF object == "classifications" %]
      分類
    [% ELSIF object == "components" %]
      コンポーネント
    [% ELSIF object == "custom_fields" %]
      カスタムフィールド
    [% ELSIF object == "field_values" %]
      フィールド値
    [% ELSIF object == "flagtypes" %]
      フラグタイプ
    [% ELSIF object == "group_access" %]
      グループ権限
    [% ELSIF object == "groups" %]
      グループ
    [% ELSIF object == "keywords" %]
      キーワード
    [% ELSIF object == "milestones" %]
      マイルストーン
    [% ELSIF object == "multiple_bugs" %]
      一度に複数の [% terms.bugs %]
    [% ELSIF object == "parameters" %]
      パラメータ
    [% ELSIF object == "products" %]
      プロダクト
    [% ELSIF object == "quips" %]
      quips
    [% ELSIF object == "reports" %]
      whine レポート
    [% ELSIF object == "sanity_check" %]
      sanity check
    [% ELSIF object == "settings" %]
      設定
    [% ELSIF object == "sudo_session" %]
      sudo セッション
    [% ELSIF object == "timetracking_summaries" %]
      時間追跡サマリーレポート
    [% ELSIF object == "user" %]
      指定したユーザ
    [% ELSIF object == "users" %]
      ユーザ
    [% ELSIF object == "versions" %]
      バージョン
    [% ELSIF object == "workflow" %]
      ワークフロー
    [% END %]

    [% IF action == "access" %]
      閲覧
    [% ELSIF action == "add" %]
      新規登録
    [% ELSIF action == "begin" %]
      開始
    [% ELSIF action == "modify" %]
      編集
    [% ELSIF action == "delete" %]
      削除
    [% ELSIF action == "edit" %]
      追加、編集、削除
    [% ELSIF action == "move" %]
      移動
    [% ELSIF action == "run" %]
      実行
    [% ELSIF action == "schedule" %]
      スケジューリング
    [% ELSIF action == "use" %]
      利用
    [% END %]

    は許可されていません。

    [% Hook.process("auth_failure") %]

  [% ELSIF error == "attachment_deletion_disabled" %]
    [% title = "添付の削除はできません" %]
    このサイトでは添付は削除できません。

  [% ELSIF error == "attachment_removed" %]
    [% title = "添付は削除されました" %]
    削除しようとした添付は既に削除されています。

  [% ELSIF error == "bug_access_denied" %]
    [% title = "アクセスは拒否されました" %]
    [% admindocslinks = {'groups.html' => 'グループセキュリティー'} %]
    [% terms.bug %] #[% bug_id FILTER html %] へのアクセス権限がありません。

  [% ELSIF error == "bug_access_query" %]
    [% title = "アクセスは拒否されました" %]
    [% docslinks = {'myaccount.html' => 'アカウント作成'} %]
    [% terms.bug %] #[% bug_id FILTER html %] へのアクセス権限がありません。
    この [% terms.bug %] を見るには、まず必要な権限を持った
    <a href="show_bug.cgi?id=
                   [% bug_id FILTER url_quote %]&amp;GoAheadAndLogIn=1">
    アカウントでログイン</a> してください。

  [% ELSIF error == "buglist_parameters_required" %]
    [% title = "必要なパラメータが足りません" %]
    [% docslinks = {'query.html' => "$terms.bugs の検索",
                    'query.html#list' => "$terms.Bug リスト"} %]
    検索語無しでは、検索や保存された検索の作成はできません。

  [% ELSIF error == "chart_too_large" %]
    [% title = "チャートが大きすぎます" %]
    チャートの最大サイズは 2000 &times; 2000 に制限されています。

  [% ELSIF error == "comment_invalid_isprivate" %]
    コメント [% id FILTER html %] の privacy を変更しようとしましたが、
    [% terms.bug %] の妥当なコメントではありません。

  [% ELSIF error == "comment_required" %]
    [% title = "コメントをつけてください" %]
    [% IF old && new %]
      [% terms.abug %] のステータスを
      [%+ old.name FILTER html %] から [% new.name FILTER html %]
      に変更する場合、<b>コメント</b>
    [% ELSIF new %]
      この [% terms.bug %] の説明
    [% ELSE %]
      この変更に関する <b>コメント</b>
    [% END %]
    を記述する必要があります。

  [% ELSIF error == "comment_too_long" %]
    [% title = "コメントが長すぎます" %]
    コメントは 65,535 文字以上にはできません。

  [% ELSIF error == "auth_classification_not_enabled" %]
    [% title = "指定された Classification は無効です" %]
    Classification は無効です。

  [% ELSIF error == "classification_not_specified" %]
    [% title = "Classification 名を指定してください" %]
    Classification 名を入力してください。

  [% ELSIF error == "classification_already_exists" %]
    [% title = "指定された Classification が既に存在しています" %]
    '[% name FILTER html %]' の Classification は既に存在します。

  [% ELSIF error == "classification_doesnt_exist" %]
    [% title = "指定された Classification は存在しません" %]
    指定した Classification '[% name FILTER html %]' は存在しません。

  [% ELSIF error == "classification_doesnt_exist_for_product" %]
    [% title = "指定された Classification は指定のプロダクトに対して存在しません" %]
    Classification '[% classification FILTER html %]' はプロダクト
    '[% product FILTER html %]' には存在しません。

  [% ELSIF error == "classification_invalid_sortkey" %]
    [% title = "Classification のソート順が無効です" %]
    Classification '[% name FILTER html %]' には ソート順として
    <em>[% sortkey FILTER html %]</em> は利用できません。
    正の整数である必要があります。

  [% ELSIF error == "classification_not_deletable" %]
    [% title = "既定の Classification は削除できません" %]
    既定の Classification は削除できません。

  [% ELSIF error == "classification_has_products" %]
    この Classification にはプロダクトが登録されています。
    削除するには、これらのプロダクトを他の Classification に移動してください。

  [% ELSIF error == "component_already_exists" %]
    [% title = "指定されたコンポーネントは既に存在しています" %]
    指定されたコンポーネント名 <em>[% name FILTER html %]</em>
    はプロダクト <em>[% product.name FILTER html %]</em>
    に既に存在しています。

  [% ELSIF error == "component_blank_description" %]
    [% title = "コンポーネントの説明を入力してください" %]
    コンポーネントの説明は必須項目です。

  [% ELSIF error == "component_blank_name" %]
    [% title = "コンポーネントの名前を入力してください" %]
    コンポーネントの名前は必須項目です。

  [% ELSIF error == "component_has_bugs" %]
    [% title = BLOCK %]指定のコンポーネントには [% terms.Bugs %] が存在します[% END %]
    このコンポーネントには [% nb FILTER html %] [%+ terms.bugs %] が存在します。
    削除するには [% terms.bugs %] を他のコンポーネントに移動してください。

  [% ELSIF error == "component_name_too_long" %]
    [% title = "コンポーネント名が長すぎます" %]
    コンポーネント名は 64 文字以上にはできません。
    '[% name FILTER html %]' は長すぎます ([% name.length %] 文字入力されています)。

  [% ELSIF error == "component_need_initialowner" %]
    [% title = "コンポーネントには既定の担当者が必要です" %]
    コンポーネントには既定の担当者が必要です。

  [% ELSIF error == "customfield_nonexistent" %]
    [% title = "未知のカスタムフィールド" %]
    未知のカスタムフィールド '[% name FILTER html %]' が存在します。

  [% ELSIF error == "customfield_not_obsolete" %]
    [% title = "カスタムフィールドが \"用済み\" に設定されていません" %]
    カスタムフィールド '[% name FILTER html %]' は "用済み" に設定されていません。
    削除する前に「用済み」に設定してください。

  [% ELSIF error == "customfield_has_activity" %]
    [% title = "指定のカスタムフィールドは利用されています" %]
    カスタムフィールド '[% name FILTER html %]' はバグの更新履歴に記録されているため、
    削除できません。

  [% ELSIF error == "customfield_has_contents" %]
    [% title = "指定のカスタムフィールドに情報が残っています" %]
    カスタムフィールド '[% name FILTER html %]' に情報を保存している
    [% terms.bug %] があるため、削除できません。

  [% ELSIF error == "dependency_loop_multi" %]
    [% title = "依存関係がループしています" %]
    次の変更が加えられた場合、
    以下の [% terms.bug %] は依存関係の "依存する" "妨害する" の両方に含まれることになります。
    [% FOREACH dep = deps %]
      [%+ dep FILTER bug_link(dep) FILTER none %]
    [% END %].
    これは依存関係のループを作ることになり、許可できません。

  [% ELSIF error == "dependency_loop_single" %]
    [% title = "依存関係のループが検出されました" %]
    [% terms.abug %] を自分自身に "依存する" もしくは "妨害する" ことはできません。

  [% ELSIF error == "dupe_id_required" %]
    [% title = "重複先の $terms.Bug ID が入力されていません" %]
    この [% terms.bug %] の重複先となる [% terms.abug %] ID が入力されていません。

  [% ELSIF error == "dupe_not_allowed" %]
    [% title = "$terms.bugs を重複にできません" %]
    複数の [% terms.bugs %] を更新する際に、[% terms.bugs %] を重複にはできません。

  [% ELSIF error == "dupe_loop_detected" %]
    [% title = "重複の関係にループがあります" %]
    [% terms.bug %] [%+ bug_id FILTER html %] を、
    [% IF dupe_of == bug_id %]
      自分自身の
    [% ELSE %]
      重複のループが出来るため
      [%+ terms.bug %] [%+ dupe_of FILTER html %]の
    [% END %]
    重複にはできません。

  [% ELSIF error == "email_change_in_progress" %]
    [% title = "メールアドレスの変更は既に申請されています" %]
    メールアドレスの変更は既に申請されています。メールボックスを確認してください。

  [% ELSIF error == "email_confirmation_failed" %]
    [% title = "メールアドレスの確認に失敗しました" %]
    メールアドレス確認の手続きが失敗に終わりました。

  [% ELSIF error == "email_no_text_plain" %]
    あなたのメッセージは空です。[% terms.Bugzilla %] は HTML のみのメールや、
    添付ファイル付きの HTML メールは受け付けません。

  [% ELSIF error == "empty_group_description" %]
    [% title = "グループの説明を入力してください" %]
    グループの説明は必須項目です。

  [% ELSIF error == "empty_group_name" %]
    [% title = "グループの名前を入力してください" %]
    グループの名前は必須項目です。
  
  [% ELSIF error == "entry_access_denied" %]
    [% title = "権限がありません" %]
    [% admindocslinks = {'groups.html' => 'グループセキュリティー'} %]
    <em>[% product FILTER html %]</em> は存在しないか、あなたはこのプロダクトに
    [% terms.abug %] を登録する権限がありません。

  [% ELSIF error == "field_already_exists" %]
    [% title = "指定されたフィールド名は既に存在しています" %]
    フィールド '[% field.name FILTER html %]' 
    ([% field.description FILTER html %]) は既に存在しています。
    別の名前を選択してください。

  [% ELSIF error == "field_invalid_name" %]
    [% title = "無効なフィールド名です" %]
    '[% name FILTER html %]' はフィールド名としては無効です。
    英文字、数字、アンダースコア (_) のみが利用可能です。

  [% ELSIF error == "field_invalid_sortkey" %]
    [% title = "フィールドのソート順が無効です" %]
    指定されたソート順 [% sortkey FILTER html %] は妥当な正の整数ではありません。

  [% ELSIF error == "field_missing_description" %]
    [% title = "フィールドの説明を入力してください" %]
    フィールドの説明は必須項目です。

  [% ELSIF error == "field_missing_name" %]
    [% title = "フィールドの名前を入力してください" %]
    フィールドの名前は必須項目です。

  [% ELSIF error == "fieldname_invalid" %]
    [% title = "指定されたフィールドは存在しません" %]
    フィールド '[% field FILTER html %]' は存在しないか、
    このインターフェースでは編集できません。

  [% ELSIF error == "fieldname_not_specified" %]
    [% title = "フィールド名が指定されていません" %]
    編集しようとするフィールドの名前が指定されていません。

  [% ELSIF error == "fieldvalue_already_exists" %]
    [% title = "指定されたフィールド値は既に存在しています" %]
    フィールド '[%- field.description FILTER html %]' には既に値として
    '[% value FILTER html %]' は存在しています。

  [% ELSIF error == "fieldvalue_doesnt_exist" %]
    [% title = "指定されたフィールド値は存在しません" %]
    フィールド '[% field FILTER html %]' には値として
    '[% value FILTER html %]' は存在しません。

  [% ELSIF error == "fieldvalue_is_default" %]
    [% title = "指定されたフィールド値は既定値です" %]
    '[% value FILTER html %]' はフィールド '[% field.description FILTER html %]'
    の既定値ですので削除できません。
    [% IF user.groups.tweakparams %]
      先に既定値を <a href="editparams.cgi?section=bugfields#
      [%- param_name FILTER url_quote %]">変更</a> する必要があります。
    [% END %]

  [% ELSIF error == "fieldvalue_name_too_long" %]
    [% title = "フィールド値が長すぎます" %]
    フィールド値は 60 文字に制限されています。
    '[% value FILTER html %]' は長すぎます ([% value.length %] 文字入力されています)。

  [% ELSIF error == "fieldvalue_not_editable" %]
    [% title = "指定されたフィールド値は編集できません" %]
    フィールド '[% field.description FILTER html %]' で特殊な役割を担っているため、
    フィールド値 '[% old_value FILTER html %]' は変更できません。

  [% ELSIF error == "fieldvalue_not_deletable" %]
    [% title = "指定されたフィールド値は削除できません" %]
    フィールド '[% field.description FILTER html %]' で特殊な役割を担っているため、
    フィールド値 '[% value FILTER html %]' は削除できません。

  [% ELSIF error == "fieldvalue_not_specified" %]
    [% title = "フィールド値が指定されていません" %]
    フィールド値を編集しようとしましたが、フィールド値が指定されていません。

  [% ELSIF error == "fieldvalue_reserved_word" %]
    [% title = "指定されたフィールド値は予約されています" %]
    フィールド '[% field.description FILTER html %]' に
    フィールド値 '[% value FILTER html %]' は使えません。
    指定された値はシステム内部で予約されています。
    他の値を選択してください。

  [% ELSIF error == "fieldvalue_sortkey_invalid" %]
    [% title = "フィールド値のソート順が無効です" %]
    フィールド '[% name FILTER html %]' に指定されたソート順
    '[% sortkey FILTER html %]' は妥当な正の整数ではありません。

  [% ELSIF error == "fieldvalue_still_has_bugs" %]
    [% title = "指定されたフィールド値は削除できません" %]
    [%+ count FILTER html %] [%+ terms.bugs %] で使用されているため、
    フィールド '[% field FILTER html %]' からフィールド値
    '[% value FILTER html %]' を削除できません。

  [% ELSIF error == "fieldvalue_undefined" %]
    [% title = "未定義のフィールド値は許可されていません" %]
    値を指定する必要があります。

